/*1. Fa√ßa um programa que leia uma string e a imprima.*/

#include <stdio.h>

int main(){

  char str[5];

  printf("Digite uma palavra de 4 letras: ");

  for (int i = 0; i < 4; i++)
  {
    scanf(" %c", &str[i]);
  }

  printf("\nA palavra digitada foi: ");

  for (int i = 0; i < 4; i++)
  {
    printf("%c", str[i]);

  } 
  
  return 0;

}
